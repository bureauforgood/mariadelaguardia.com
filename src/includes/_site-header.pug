header.site-header
	.site-header__wrapper
		a.site-header__head.animsition-link(href='/') #{siteName}
		a.link.site-header__link(href='mailto:'+contactEmail) #{contactEmail}

	if bodyClass === 'home'
		nav.navigation-links
			a.link.link--nav.js-link--nav(href='#about') About Me
